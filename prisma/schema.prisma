generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model article {
  //defaults
  id      String   @id @default(uuid())
  created DateTime @default(now())
  updated DateTime @updatedAt

  //props
  title String
  body  String
  tags  String[]
}
